package ru.averinaoy.api.lesson1;

import java.util.HashSet;
import java.util.Objects;

public class task01 {
    private String name;
    private int age;

    public task01(String name, int age){
        this.name = name;
        this.age = age;
    }

    @Override
    public boolean equals (Object o){
        if (this == o) return true;
        if (o == null || getClass() !=o.getClass()) return false;
        task01 cat = (task01)o;
        return age == cat.age &&
                Object.equals(name,cat.name);
    }

    @Override
    public int hashCode(){
        return Objects.hash(name, age);
    }

    public String getName() {
        return name;
    }

    public int getAge() {
        return age;
    }

    public static void main(String[] args) {
        HashSet<task01> catSet = new HashSet<>();
        task01 cat1 = new task01("Barsic", 1);
        task01 cat2 = new task01("Murzik", 5);
        task01 cat3 = new task01("Veterok", 8);
        task01 cat4 = new task01("Puchok", 3);
        task01 cat5 = new task01("Barsic", 1);
        catSet.add(cat1);
        catSet.add(cat2);
        catSet.add(cat3);
        catSet.add(cat4);
        catSet.add(cat5);
        for (task01 cat : catSet){
            System.out.println(cat.getName() + ":" + cat.getAge());
        }
    }
}

